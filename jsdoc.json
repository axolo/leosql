{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc",
      "closure"
    ]
  },
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)(_|node_modules)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs",
    "recurse": true
  },
  "docdash": {
    "search": true,
    "collapse": true,
    "menu": {
      "GitHub": {
        "href": "https://github.com/axolo/leosql",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
